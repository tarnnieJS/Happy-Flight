<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- White vertical line -->
    <item android:left="0dp">
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@color/red_primary" />
        </shape>
    </item>

    <!-- Gray vertical line -->
    <item android:left="0dp">
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@color/red_primary" />
        </shape>
    </item>

    <!-- White vertical line -->
    <item android:left="0dp">
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@color/red_primary" />
        </shape>
    </item>

</layer-list>
